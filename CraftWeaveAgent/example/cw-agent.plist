<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.xconfig.cw-agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/cw-agent</string>
        <string>daemon</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/var/lib/cw-agent</string>
    <key>StandardOutPath</key>
    <string>/tmp/cw-agent.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/cw-agent.err</string>
</dict>
</plist>
